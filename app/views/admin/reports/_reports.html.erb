<table class="table table-striped table-bordered" id="report">
  <thead>
    <th>
      <% @report.header[1].each do |header| %>
        <td><%= header.to_s %></td>
      <% end %>
    </th>
  </thead>
  <tbody>
    <% @report.header[0].each do |r_header| %>
      <tr>
        <td><%= r_header.to_s %></td>
        <% @report.header[1].each do |c_header| %>
          <td>
            <%= print_report_cell @report.body[r_header][c_header] %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>